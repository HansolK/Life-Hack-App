  <div class="list_ideas_page">

    <%@ideas.each do |idea|%>
    <%user=User.find_by(id: idea.user_id)%>
      <div>
      <a href="/ideas/detail/<%=idea.id%>">
      <p><%=idea.title%></p>
      </a>
      <p>written by: <%=user.name%></p>
      <p>written at: <%=idea.created_at%></p>
      
      </div>
    <%end%>
  </div>